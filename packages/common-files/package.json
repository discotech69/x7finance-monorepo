{
  "name": "common-files",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "clean": "rm -rf dist",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "lint": "eslint \"src/**/*.ts*\""
  },
  "devDependencies": {
    "@types/jest": "29.5.0",
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "eslint": "8.38.0",
    "eslint-config-custom": "workspace:*",
    "jest": "29.5.0",
    "jest-presets": "workspace:*",
    "react": "18.2.0",
    "tsconfig": "workspace:*",
    "tsup": "6.7.0",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "react-dom": "18.2.0"
  }
}
